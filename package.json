{
	"name": "huashui",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"lint:development": "cross-env NODE_ENV=development eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
		"lint:production": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
		"prettier": "prettier --write .",
		"prepare": "husky install",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^0.2.7",
		"axios": "^0.25.0",
		"element-plus": "^2.0.1",
		"vue": "^3.2.30",
		"vue-router": "^4.0.12",
		"vuex": "^4.0.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.1.0",
		"@commitlint/config-conventional": "^16.0.0",
		"@types/node": "^17.0.15",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"@vitejs/plugin-vue": "^2.1.0",
		"@vue/compiler-sfc": "^3.2.30",
		"consola": "^2.15.3",
		"conventional-changelog-cli": "^2.2.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.8.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.4.1",
		"husky": "^7.0.4",
		"less": "^4.1.2",
		"lint-staged": "^12.3.3",
		"mrm": "^3.0.10",
		"prettier": "^2.5.1",
		"sass": "^1.49.7",
		"typescript": "^4.5.5",
		"typescript-eslint": "^0.0.1-alpha.0",
		"unplugin-auto-import": "^0.5.11",
		"unplugin-vue-components": "^0.17.17",
		"vite": "^2.7.13",
		"vite-plugin-style-import": "^2.0.0",
		"vue-tsc": "^0.29.8"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx,vue,js,jsx}": [
			"pnpm lint:development",
			"pnpm prettier",
			"git add "
		]
	}
}
